{
  "name": "weareyou",
  "description": "We are you - public website",
  "version": "0.0.1",
  "private": true,
  "license": "Apache-2.0",
  "config": {
    "appName": "weareyou",
    "rootPlaceholders": [
      "jss-header",
      "jss-main",
      "jss-footer"
    ],
    "sitecoreDistPath": "/dist/weareyou",
    "sitecoreConfigPath": "/App_Config/Include/zzz",
    "graphQLEndpointPath": "/api/weareyou",
    "language": "nl-NL"
  },
  "author": {
    "name": "We are you",
    "url": "https://www.weareyou.com"
  },
  "scripts": {
    "jss": "jss",
    "start": "scjss-verify-setup && npm-run-all --serial bootstrap:connected start:react start:watch-components",
    "build": "CI=true npm-run-all --serial bootstrap:connected build:client build:server",
    "test": "build-storybook && percy-storybook --widths=320,1280",
    "scaffold": "node scripts/scaffold-component.js",
    "start:react": "node scripts/start.js",
    "start:proxy": "node scripts/disconnected-mode-proxy.js",
    "start:watch-components": "node scripts/generate-component-factory.js --watch",
    "build:client": "cross-env-shell PUBLIC_URL=$npm_package_config_sitecoreDistPath \"node scripts/build.js\"",
    "build:server": "cross-env-shell NODE_ENV=development \"webpack --config server/server.webpack.config.js\"",
    "bootstrap:connected": "node scripts/bootstrap.js",
    "bootstrap:disconnected": "node scripts/bootstrap.js --disconnected",
    "graphql:update": "node -r babel-register ./scripts/update-graphql-fragment-data.js",
    "storybook": "start-storybook -p 9001",
    "proxy": "npm-run-all bootstrap:connected start:react start:proxy start:watch-components"
  },
  "browserslist": [
    ">5%",
    "last 2 versions",
    "Firefox ESR",
    "IE 11"
  ],
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@sitecore-jss/sitecore-jss-react": "^11.0.0",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-knobs": "^5.1.9",
    "@storybook/addon-options": "^5.1.9",
    "@storybook/addons": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "apollo-cache-inmemory": "^1.2.1",
    "apollo-client": "^2.3.1",
    "apollo-link": "^1.2.2",
    "apollo-link-batch-http": "^1.2.2",
    "apollo-link-http": "^1.5.4",
    "apollo-link-persisted-queries": "^0.2.0",
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "chalk": "^2.4.1",
    "classnames": "^2.2.6",
    "css-loader": "^3.0.0",
    "dotenv": "^8.0.0",
    "dotenv-expand": "^5.1.0",
    "file-loader": "^4.0.0",
    "fs-extra": "^8.0.1",
    "graphql": "^14.4.0",
    "graphql-tag": "^2.10.1",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "i18next": "^17.0.4",
    "i18next-fetch-backend": "^2.2.0",
    "intersection-observer": "^0.7.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^24.8.0",
    "lazysizes": "^5.1.0",
    "lottie-web": "^5.5.5",
    "object-assign": "4.1.1",
    "object-fit-images": "^3.2.4",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^8.0.1",
    "postcss-pxtorem": "^4.0.1",
    "promise": "8.0.2",
    "prop-types": "^15.7.2",
    "raf": "3.4.1",
    "react": "^16.8.6",
    "react-apollo": "^2.5.8",
    "react-dev-utils": "^9.0.1",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-i18next": "^10.11.2",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^5.0.1",
    "react-transition-group": "^4.1.1",
    "resolve": "^1.11.1",
    "serialize-javascript": "^2.1.1",
    "shortid": "^2.2.14",
    "storybook-react-router": "^1.0.5",
    "style-loader": "^0.23.1",
    "svgxuse": "^1.2.6",
    "sw-precache-webpack-plugin": "^0.11.5",
    "swiper": "^4.5.0",
    "universal-cookie": "^3.1.0",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpack-manifest-plugin": "^2.0.4",
    "whatwg-fetch": "3.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@percy/storybook": "^3.1.0",
    "@sitecore-jss/sitecore-jss-cli": "^11.0.0",
    "@sitecore-jss/sitecore-jss-dev-tools": "^11.0.0",
    "@sitecore-jss/sitecore-jss-manifest": "^11.0.0",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "blocss": "^7.0.0",
    "chokidar": "^2.0.3",
    "cross-env": "^5.1.5",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.7.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.14.2",
    "html-loader": "^0.5.5",
    "inline-manifest-webpack-plugin": "^4.0.2",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "null-loader": "^0.1.1",
    "open-browser-webpack-plugin": "^0.0.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "percy": "^0.7.2",
    "pnp-webpack-plugin": "^1.5.0",
    "postcss-preset-env": "^6.6.0",
    "postcss-safe-parser": "^4.0.1",
    "sass-loader": "^7.1.0",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-scss": "^3.8.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "svg-sprite-loader": "^4.1.6",
    "svgo": "^1.2.2",
    "svgo-loader": "^2.2.1",
    "terser-webpack-plugin": "^1.3.0",
    "url-loader": "^2.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  }
}
